<!-- /.row -->
<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                Upload Images
            </div>
            <!-- /.panel-heading -->
            <div class="panel-body">
            <%= form_for @category, html: { class: 'form-horizontal', multipart: true } do |f| %>
              <div class="control-group">
                <div class="field">
                  <%= f.label "Add multiple Images", class: 'control-label' %>
                  <%= file_field_tag "images[]", type: :file, multiple: true %>
                </div>
              </div>
              <br />
              <div class="form-actions">
                <%= f.submit nil, class: 'btn btn-primary' %>
                <%= link_to t('.cancel', default: t("helpers.links.cancel")),
                            categories_path, class: 'btn btn-mini' %>
              </div>
            <% end %>
            </div>
            <!-- /.panel-body -->
        </div>
        <!-- /.panel -->
    </div>
    <!-- /.col-lg-12 -->
</div>
<!-- /.row -->










