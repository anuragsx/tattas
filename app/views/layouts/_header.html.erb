<header id="masthead" class="site-header centered container">
  <div class="top-panel">
    <div class="top-panel__wrap container">
      <div class="top-panel__search">
        <div class="search-form_wrap">
          <form role="search" method="get" class="search-form" action="">
            <label>
            <span class="screen-reader-text">Enter keyword</span>
            <input type="search" class="search-form__field"
              placeholder="Enter keyword"
              value="" name="s"
              title="Enter keyword" />
            </label>
            <button type="submit" class="search-form__submit btn">Search</button>
          </form>
          <span class="close-btn"><a href="#" class="close-btn-link"><i class="material-icons">close</i></a></span>
          <span class="open-btn"><a href="#" class="open-btn-link"><i class="material-icons">search</i></a></span>                        
        </div>
      </div>
      <div class="social-list social-list--header social-list--icon">
        <ul id="social-list-1" class="social-list__items inline-list">
          <li>
            <a href="https://www.facebook.com/TemplateMonster/">
            <span class="screen-reader-text">Facebook</span><span class="animation-block"></span>
            </a>
          </li>
          <li>
            <a href="https://twitter.com/TemplateMonster">
            <span class="screen-reader-text">Twitter</span><span class="animation-block"></span></a>
          </li>
          <li>
            <a href="https://plus.google.com/+TemplateMonster">
            <span class="screen-reader-text">Google +</span><span class="animation-block"></span></a>
          </li>
          <li>
            <a href="https://www.instagram.com/template_monster/">
            <span class="screen-reader-text">Instagram</span><span class="animation-block"></span></a>
          </li>
          <li>
            <a href="https://www.pinterest.com/templatemonster/">
            <span class="screen-reader-text">Pinterest</span><span class="animation-block"></span></a>
          </li>
        </ul>
      </div>
    </div>
  </div>
  <!-- .top-panel -->                
  <div class="header-container">
