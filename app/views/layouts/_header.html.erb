      <div><%=h @header_ad.html_safe %></div>
      <div class="site-branding">
        <a class="site-logo__link" href="/index" rel="home">
          <div class="site-logo image"><img src="<%= asset_path('logo.png') %>" alt="Mezmerize" class="site-link__img" ></div>
          <div class="site-description">Fashion Brand Lookbook</div>
        </a>
